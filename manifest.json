{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Discord Meet Callout",
  "version": "1.3.1",
  "description": "By Dark Web",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Discord Meet Callout",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://teams.microsoft.com/*", "https://meet.google.com/*", "https://*.zoom.us/*"],
    "js": ["jquery-3.5.1.js", "contentscriptnith.js"]
  }],
  "permissions": [
    "storage",
    "activeTab"
  ]
}